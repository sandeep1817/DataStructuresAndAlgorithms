package com.algorithms.arrays;

import java.util.Scanner;

public class SumOfSeries {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int sum=0;
		int num = sc.nextInt();
		for(int i=0;i<=5;i++){
			sum = sum+i;
		}
		System.out.println("SUme of the given series: "+sum);
	}

}
